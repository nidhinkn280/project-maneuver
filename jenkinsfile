pipeline {
    agent any

    stages {
        stage('Stage 1') {
            steps {
                script {
                    // Execute first Python script
                    sh 'python Automation.py'
                }
            }
        }
        stage('Stage 2') {
            steps {
                script {
                    // Execute second Python script
                    sh 'python Slack.py'
                }
            }
        }
    }
}
